{
  "name": "javascript-server",
  "version": "1.0.0",
  "description": "testing functionalities",
  "main": "index.js",
  "scripts": {
    "dev": "babel-node extra --presets env",
    "src": "npm run clean && tsc-watch --project . --onSuccess \"npm run nodemon \"",
    "clean": "rm -rf dist/*",
    "nodemon": "nodemon ./dist/index.js",
    "pre-commit": "npm run src",
    "lint": "./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json --exclude 'node_modules/**/*.{j,t}s'",
    "lint:fix": "yarn lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akashdutt/javascript-server.git"
  },
  "author": "AKASH DUTT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akashdutt/javascript-server/issues"
  },
  "homepage": "https://github.com/akashdutt/javascript-server#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/mongoose": "^5.3.11",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.1",
    "nodemon": "^1.18.9",
    "tsc-watch": "^1.1.36",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.8",
    "tslint": "^5.12.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  }
}
